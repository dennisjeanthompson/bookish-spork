{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm install && vite build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://bookish-spork-api.onrender.com/api/:path*"
    }
  ]
}
